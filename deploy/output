# Source: go-links/templates/tests/test-connection.yaml
apiVersion: v1
kind: Pod
metadata:
  name: "golinks-go-links-test-connection"
  labels:
    helm.sh/chart: go-links-0.1.0
    app.kubernetes.io/name: go-links
    app.kubernetes.io/instance: golinks
    app.kubernetes.io/version: "1.16.0"
    app.kubernetes.io/managed-by: Helm
  annotations:
    "helm.sh/hook": test
spec:
  containers:
    - name: wget
      image: busybox
      command: ['wget']
      args: ['golinks-go-links:80']
  restartPolicy: Never

