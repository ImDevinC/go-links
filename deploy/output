# Source: go-links/templates/service.yaml
apiVersion: v1
kind: Service
metadata:
  name: golinks-go-links
  labels:
    helm.sh/chart: go-links-0.1.0
    app.kubernetes.io/name: go-links
    app.kubernetes.io/instance: golinks
    app.kubernetes.io/version: "1.16.0"
    app.kubernetes.io/managed-by: Helm
spec:
  type: ClusterIP
  ports:
    - port: 80
      targetPort: http
      protocol: TCP
      name: http
  selector:
    app.kubernetes.io/name: go-links
    app.kubernetes.io/instance: golinks

---
# Source: go-links/templates/deployment.yaml
apiVersion: apps/v1
kind: Deployment
metadata:
  name: golinks-go-links
  labels:
    helm.sh/chart: go-links-0.1.0
    app.kubernetes.io/name: go-links
    app.kubernetes.io/instance: golinks
    app.kubernetes.io/version: "1.16.0"
    app.kubernetes.io/managed-by: Helm
spec:
  replicas: 1
  selector:
    matchLabels:
      app.kubernetes.io/name: go-links
      app.kubernetes.io/instance: golinks
  template:
    metadata:
      labels:
        app.kubernetes.io/name: go-links
        app.kubernetes.io/instance: golinks
    spec:
      serviceAccountName: golinks-go-links
      securityContext:
        {}
      containers:
        - name: go-links
          securityContext:
            {}
          image: "ghcr.io/imdevinc/go-links:latest"
          imagePullPolicy: IfNotPresent
          ports:
            - name: http
              containerPort: 80
              protocol: TCP
          livenessProbe:
            httpGet:
              path: /
              port: http
          readinessProbe:
            httpGet:
              path: /
              port: http
          resources:
            {}
